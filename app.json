{
  "expo": {
    "name": "Club 420 Poker",
    "slug": "club420poker",
    "scheme": "club420poker",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.club420.poker"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.club420.poker"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "backgroundColor": "#000000",
      "themeColor": "#f59e0b"
    }
  }
}
